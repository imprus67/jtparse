generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../../telegram.sqlite"
}

model Message {

  id Int @id @default(autoincrement())
  messageType String
  messageText String
  entities Json
  messageMedia Boolean 
  spoiler Boolean @default(false)
  video Boolean @default(false)
  round Boolean @default(false)
  voice Boolean @default(false)
  attributesOfDocument Json
  webPage Json 
  mediaPath String
  contact Json
  fwdFrom Json 
  isGrouped Boolean @default(false)
  groupId String @default("null")
  fromChannel String
  isSend Boolean @default(false)
  createdAt DateTime @default(now())
}

