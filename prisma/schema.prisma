generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../telegram.sqlite"
}

model Message {

  id Int @id @default(autoincrement())
  messageType String
  messageText String
  messageMedia Boolean 
  fwdFrom Json 
  createdAt DateTime @default(now())
}

